require '~/projects/airport_challenge/lib/airport.rb'
require '~/projects/airport_challenge/lib/plane.rb'

airport = Airport.new
plane = Plane.new

airport.land_plane(plane)
airport.instance_variable_get(:@planes_at_airport)
airport.take_off(plane)
airport.instance_variable_get(:@planes_at_airport)
airport.set_capacity(10)